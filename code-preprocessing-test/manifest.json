{
  "name": "code preprocessing test",
  "version": "0.1.0",
  "manifest_version": 2,
  "minimum_chrome_version": "50.0",
  "background": {
    "scripts": ["background.js"]
  },
  "content_scripts": [

  ],
  "icons": {
   "128": "icon.png"
 },
 "browser_action": {
   "name": "Run FromJS on this page"
 },
  "permissions": [
      "webRequest",
      "webRequestBlocking",
      "http://*/*",
      "https://*/*",
      "file://*/*"
  ],
  "web_accessible_resources": [
      "injected.js",
      "inspector.js",
      "resolveFrameWorker.js",
      "inhibitJavaScriptExecution.js"
  ]
}
