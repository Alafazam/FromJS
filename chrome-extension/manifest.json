{
  "name": "FromJS",
  "version": "1.0",
  "manifest_version": 2,
  "minimum_chrome_version": "50.0",
  "background": {
    "scripts": ["background.js"]
  },
  "content_scripts": [
    {
      "matches": ["http://*/*", "https://*/*", "file://*/*"],
      "css": ["fromjs.css"],
      "run_at": "document_start"
    }
  ],
  "browser_action": {
    "name": "Run FromJS here"
  },
  "permissions": [
      "webRequest",
      "webRequestBlocking",
      "http://*/*",
      "https://*/*",
      "file://*/*"
  ],
  "web_accessible_resources": ["from.js"]
}
