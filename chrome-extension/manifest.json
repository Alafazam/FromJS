{
  "name": "FromJS",
  "version": "1.0",
  "manifest_version": 2,
  "minimum_chrome_version": "50.0",
  "background": {
    "scripts": ["background.js"]
  },
  "content_scripts": [

  ],
  "browser_action": {
    "name": "Run FromJS here"
  },
  "icons": {
   "128": "icon.png"
 },
  "permissions": [
      "webRequest",
      "webRequestBlocking",
      "http://*/*",
      "https://*/*",
      "file://*/*"
  ],
  "web_accessible_resources": ["from.js", "injected.js"]
}
